{
  "categories": [
    "Other",
    "Tools"
  ],
  "env": [
    {
      "name": "PUID",
      "label": "PUID",
      "default": "1000",
      "description": "for UserID"
    },
    {
      "name": "PGID",
      "label": "PGID",
      "default": "1000",
      "description": "for GroupID"
    },
    {
      "name": "TZ",
      "label": "TZ",
      "default": "America/Chicago",
      "description": "Specify a timezone to use for example America/New_York"
    }
  ],
  "command": ["--joal-conf=/portainer/Files/AppData/Joal/config", "--spring.main.web-environment=true", "--server.port=7073", "--joal.ui.path.prefix=SECRET_OBFUSCATION_PATH", "--joal.ui.secret-token=SECRET_TOKEN"],
  "description": "An open source command line RatioMaster with an optional WebUI.",
  "image": "anthonyraymond/joal:latest",
  "logo": "https://raw.githubusercontent.com/anthonyraymond/joal-desktop/master/resources/icon.png",
  "name": "Joal",
  "officialDoc": "https://github.com/anthonyraymond/joal#2-run-with-docker",
  "platform": "linux",
  "ports": [
    "7073:7073/tcp"
  ],
  "restart_policy": "unless-stopped",
  "title": "Joal",
  "type": 1,
  "volumes": [
    {
      "bind": "/portainer/Files/AppData/Joal/config",
      "container": "/data"
    }
  ],
  "webpage": "https://github.com/anthonyraymond/joal"
}
